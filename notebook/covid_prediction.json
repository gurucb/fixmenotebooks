				"source": [
					"from sklearn.tree import DecisionTreeClassifier\r\n",
					"from sklearn.model_selection import train_test_split\r\n",
					"from sklearn.tree import plot_tree\r\n",
					"from sklearn import metrics"
				],
				"execution_count": 17
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
