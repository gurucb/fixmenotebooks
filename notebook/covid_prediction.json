					"nteract": {
						"transient": {
							"deleting": false
						}
					}
				},
				"source": [
					"plot_tree(dt, filled=True)\r\n",
					"plt.title(\"Decision tree trained on all the Covid Data\")\r\n",
					"plt.show()\r\n",
					"# plt.savefig() GIVE THE PATH"
				],
				"execution_count": 19
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
